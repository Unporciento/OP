{
  "version": 2,
  "builds": [
    {
      "src": "api/askGemini.js",
      "use": "@vercel/node"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/askGemini",
      "dest": "/api/askGemini.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/askGemini.js": {
      "maxDuration": 60
    }
  }
}